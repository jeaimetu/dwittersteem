				var hBoardId = $("#frmReplyUpdate #postid").val();
				if ( 1 < $("input[name='hBoardId'][value='" + hBoardId + "']").length ){
					var idx = $("input[name='hBoardId']").index($("input[name='hBoardId'][value='" + hBoardId + "']").eq(0));
