		function fnImageDelete(obj){
			var imgArray = $("img[name='imgThumbNail']");
			var hideArray = $("input[name='imgUrl']");
			var idx = imgArray.index(obj);
			imgArray.eq(idx).remove();
			hideArray.eq(idx).remove();
